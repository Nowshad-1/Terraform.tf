resource "local_file" "name" {
    filename = var.filename
    content = var.content
  
}
resource "random_pet" "petname" {
    prefix = "MR"
    separator = "."
    length = "1"
}
variable "filename" {
    default = "pet.txt"
    type = string

}
variable "content" {
    default = "I Love CATS"
    type = string
}
